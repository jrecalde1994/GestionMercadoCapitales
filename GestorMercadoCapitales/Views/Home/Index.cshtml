@{
    ViewData["Title"] = "Menu principal";
}
@model CotizacionAccion


<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Ticker</th>
            <th scope="col">Precio</th>
            <th scope="col">Var.%</th>
            <th scope="col">Precio A</th>
            <th scope="col">C. Compra</th>
            <th scope="col">P. Compra</th>
            <th scope="col">P. Venta</th>
            <th scope="col">C. Venta</th>
            <th scope="col">Min-Max</th>
            <th scope="col">Vol. Num.</th>
            <th scope="col">Monto</th>
            <th scope="col">Oper</th>
            <th scope="col">Hora</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td scope="row">@Html.DisplayFor(model => model.Ticker)</td>
            <td>@Html.DisplayFor(model => model.Precio)</td>
            <td>@Html.DisplayFor(model => model.VarPorcentual)%</td>
            <td>@Html.DisplayFor(model => model.PrecioA)</td>
            <td>@Html.DisplayFor(model => model.CCompra)</td>
            <td>@Html.DisplayFor(model => model.PCompra)</td>
            <td>@Html.DisplayFor(model => model.PVenta)</td>
            <td>@Html.DisplayFor(model => model.CVenta)</td>
            <td>@Html.DisplayFor(model => model.Min) <input type="range" min="@Html.DisplayFor(model => model.Min)" max="@Html.DisplayFor(model => model.Max)" disabled> @Html.DisplayFor(model => model.Max)</td>
            <td>@Html.DisplayFor(model => model.VolNom)</td>
            <td>$@Html.DisplayFor(model => model.Monto)K</td>
            <td>@Html.DisplayFor(model => model.Oper)</td>
            <td>@Html.DisplayFor(model => model.Hora)</td>
        </tr>
    </tbody>
</table>
